{
  "name": "fast-mhtml",
  "version": "2.1.0",
  "description": "Parses mhtml files",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "nyc --reporter=html mocha src/*.test.js",
    "lint": "eslint src/*.js",
    "lint-fix": "eslint src/*.js --fix",
    "benchmark": "node --expose-gc src/benchmark/benchmark.js",
    "build": "rollup -c",
    "serve": "node src/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testimio/mhtml-parser.git"
  },
  "keywords": [
    "Parses",
    "mhtml",
    "files"
  ],
  "author": "benji@testim.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/testimio/mhtml-parser/issues"
  },
  "homepage": "https://github.com/testimio/mhtml-parser#readme",
  "dependencies": {
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.10",
    "css-tree": "^1.0.4",
    "express": "^4.17.2",
    "filenamify": "^4.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-inject": "^5.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^1.3.1",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "rollup": "^3.20.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint"
    }
  }
}
